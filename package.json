{
  "name": "datadeck-nwjs",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "clean": "cross-env del dist build",
    "copy": "mkdir dist && cp package.json dist/ && cp -rp src/assets dist/",
    "dist": "npm run copy && cd dist && npm install --production && cd .. && npm run compile-ui && npm run compile-js",
    "build-osx": "cross-env NODE_ENV=production nwb nwbuild --production --side-by-side --mac-icns ./dist/assets/dd.icns -p osx64 -v 0.19.4 -o ./build ./dist",
    "build-linux": "cross-env NODE_ENV=production nwb nwbuild --production --side-by-side -p linux64 -v 0.19.4 -o ./build ./dist",
    "release-linux": "npm run quality && npm run clean && npm run dist && npm run build-linux",
    "release-osx": "npm run quality && npm run clean && npm run dist && npm run build-osx",
    "debug-osx": "cross-env NODE_ENV=development nwb nwbuild --mac-icns ./dist/assets/dd.icns -p osx64 -v 0.19.4-sdk -r -- --remote-debugging-port=9222 ./dist",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "jshint": "cross-env NODE_ENV=production jshint --reporter=./node_modules/jshint-stylish ./src",
    "eslint": "cross-env eslint ./src",
    "quality": "npm run jshint && npm run eslint",
    "babel": "cross-env NODE_ENV=production babel ./src/js/backend --out-dir ./dist",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "compile-js": "npm run babel && npm run webpack",
    "pug": "cross-env NODE_ENV=production pug -o ./dist ./src/pug/index.pug",
    "theme": "cross-env NODE_ENV=production et -c src/less/variables.css -o dist/theme/",
    "less": "cross-env NODE_ENV=production lessc ./src/less/main.less ./dist/main.css",
    "compile-ui": "npm run pug && npm run theme && npm run less"
  },
  "js-flags": "--harmony",
  "main": "index.html",
  "window": {
    "title": "Chamberlab Datadeck",
    "icon": "assets/icon_256x256.png",
    "toolbar": true,
    "frame": true,
    "width": 1280,
    "min-width": 960,
    "height": 790,
    "min-height": 480
  },
  "author": "A. Koch",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "del-cli": "^0.2.1",
    "element-theme": "^0.5.0",
    "element-theme-default": "git+https://github.com/ElementUI/theme-default.git",
    "eslint": "^3.14.1",
    "file-loader": "^0.10.0",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "nwjs-builder": "^1.14.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "style-loader": "^0.13.1",
    "trash-cli": "^1.4.0",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "dependencies": {
    "chamberlib": "git+https://github.com/Chamberlab/node-chamberlib.git",
    "chart.js": "^2.4.0",
    "element-ui": "^1.1.6",
    "feathers": "^2.0.3",
    "feathers-client": "^1.9.0",
    "feathers-hooks": "^1.7.1",
    "feathers-socketio": "^1.4.2",
    "moment": "^2.17.1",
    "socket.io-client": "^1.7.2",
    "through2": "^2.0.3",
    "vue": "^2.1.8",
    "vue-chartjs": "^2.3.3"
  }
}
